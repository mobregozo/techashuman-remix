{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/build/server/index.js" }],
  "rewrites": [{ "source": "/(.*)", "destination": "/build/server/index.js" }]
}
